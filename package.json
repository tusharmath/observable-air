{
  "name": "observable-air",
  "description": "Observables for the calorie conscious",
  "main": "./.dist/src/main",
  "jsnext:main": "./.dist/main-es.min",
  "typings": "./.dist/src/main.d.ts",
  "scripts": {
    "benchmark": "tsc && node ./.dist/benchmarks/run",
    "cleanup": "find ./src -type f -name '*.js' -delete && find ./src -type f -name '*.map' -delete",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "hydra": "node --trace-hydrogen --trace-phase=Z --trace-deopt --code-comments --hydrogen-track-positions --redirect-code-traces --redirect-code-traces-to=code.asm .dist/benchmarks/run",
    "prepublish": "tsc -d && npm run build && npm run build:min",
    "rfc": "node chore/rfc",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "tsc -d && ava .dist/test",
    "test:watch": "ava .dist/test --watch",
    "build": "npm run build:es && npm run build:cjs",
    "build:min": "npm run build:es:min && npm run build:cjs:min",
    "build:es": "rollup -c",
    "build:es:min": "babel .dist/main-es.js > .dist/main-es.min.js",
    "build:cjs": "browserify .dist/src/main.js > .dist/main-cjs.js",
    "build:cjs:min": "babel .dist/main-cjs.js > .dist/main-cjs.min.js",
    "doc": "tsc --module es6 --target es6 --outDir .dist/es6 src/main.ts && esdoc",
    "gh-pages": "node ./.dist/scripts/gh-pages.js",
    "postpublish": "npm run doc && npm run gh-pages"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-cli": "^6.16.0",
    "babel-preset-babili": "^0.0.11",
    "benchmark": "^2.1.2",
    "browserify": "^14.1.0",
    "cli-table2": "^0.2.0",
    "commentator": "^1.0.5",
    "coveralls": "^2.11.14",
    "cz-conventional-changelog": "^2.0.0",
    "esdoc": "^0.5.2",
    "gh-pages": "^0.12.0",
    "ghooks": "^2.0.0",
    "nyc": "^10.0.0",
    "request-promise": "^4.1.1",
    "rollup": "^0.41.2",
    "rollup-plugin-commonjs": "^8.0.0",
    "semantic-release": "^6.3.2",
    "typescript": "^2.0.3",
    "uglify": "^0.1.5",
    "validate-commit-msg": "^2.8.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tusharmath/observable-air.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    }
  }
}
